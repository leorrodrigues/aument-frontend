{
  "presets": ["next/babel", "@babel/preset-typescript"],
  "plugins": [
  [
    "babel-plugin-styled-components", {
      "ssr": true,
      "displayName": true
    }
  ]
],
"env": {
  "dev": {
    "presets": ["next/babel"],
    "plugins": [
      ["import", { "libraryName": "antd", "style": true }],
      [
        "babel-plugin-styled-components", {
          "ssr": false,
          "displayName": false
        }
      ]
    ]
  }
}
}
